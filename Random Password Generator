#include <iostream>
#include <random>
using namespace std;

int main(){
    string chars="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
    random_device rd; mt19937 gen(rd());
    uniform_int_distribution<> d(0,chars.size()-1);
    for(int i=0;i<12;i++)
        cout<<chars[d(gen)];
}
